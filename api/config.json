{
  "providers": {
    "echoServ": {
      "data": {
        "url": "https://echo-serv.tbxnet.com",
        "header": {
          "authorization": "Bearer aSuperSecretKey"
        },
        "actions" : {
            "file": {
                "method": "GET",
                "path": "/v1/secret/file"
            },
            "listFiles": {
                "method": "GET",
                "path": "/v1/secret/files"
            }
        }
      }
    }
  }
}